{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": 0,
        "eol-last": 0,
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "space-before-blocks": "off",
        "indent": "off",
        "keyword-spacing": "off",
        "no-trailing-spaces": "off",
        "object-curly-spacing": "off",
        "quotes": "off",
        "operator-linebreak" : "off",
        "padded-blocks" : "off"

    },
    "extends": ["eslint:recommended", "google"]
}